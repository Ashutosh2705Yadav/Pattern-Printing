#include <iostream>
using namespace std;
void print11(int n){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=i;j++){
            if(i%2!=0){
                if(j%2!=0) cout<<1;
                else cout<<0;
            }
            if(i%2==0){
                if(j%2==0) cout<<1;
                else cout<<0;
            }}
cout<<endl;
}
}
int main() {
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;
        print11(n);
    }
    
    
    return 0;
}
